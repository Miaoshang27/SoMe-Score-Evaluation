    "import math\n",
    "import pandas as pd\n",
    "\n",
    "def m_ge(x,t,scale = 1.0):\n",
    "    return (x-t)/scale\n",
    "\n",
    "def m_l(x,t,scale = 1.0):\n",
    "    return (t-x)/scale\n",
    "\n",
    "def m_range(x,lo,hi,scale = 1.0):\n",
    "    return min(x-lo, hi-x)/scale\n",
    "\n"
